<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="group2.netapp.ProfileFragment">

    <!-- TODO: Update blank fragment layout -->
    <!--  <TextView android:layout_width="match_parent" android:layout_height="match_parent"
          android:text="@string/hello_blank_fragment" />-->

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.5"
            android:background="@drawable/back2"


            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/emailText"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="30dp"
                android:textStyle="italic"
                android:text="email"
                android:textColor="#ff3d413c" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="name"
                android:textStyle="bold"
                android:id="@+id/nameText"
                android:layout_marginBottom="5dp"
                android:layout_above="@id/emailText"
                android:layout_centerHorizontal="true" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView2"
                android:layout_above="@id/nameText"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="20dp"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.5"
            android:background="@drawable/back3">


            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="phone"
                android:ems="10"
                android:id="@+id/editText3"
                android:textSize="20dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:layout_alignParentEnd="false"
                android:layout_alignParentLeft="false"
                android:layout_alignParentRight="false"
                android:layout_alignParentStart="false"
                android:hint="Phone number"
                android:shadowColor="@color/black_overlay"
                android:textAlignment="center"
                android:textIsSelectable="false" />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPostalAddress"
                android:ems="10"
                android:textSize="20dp"
                android:id="@+id/editText4"
                android:layout_below="@id/editText3"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:layout_alignParentEnd="false"
                android:layout_alignParentRight="false"
                android:layout_alignParentLeft="false"
                android:layout_alignParentStart="false"
                android:hint="Address"
                android:maxLines="5"
                android:nestedScrollingEnabled="true"
                android:textAlignment="center" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Customer Rating"
                android:id="@+id/textView3"
                android:layout_below="@+id/textView2"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:textColor="#ff707070"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Service Rating"
                android:id="@+id/textView2"
                android:layout_below="@id/editText4"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="20dp"

                android:textColor="#ff707070"
                android:layout_marginLeft="10dp"
                android:layout_alignRight="@id/textView3"
                android:textStyle="bold" />



            <RatingBar
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ratingBar5"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_alignTop="@id/textView2"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                android:layout_alignParentEnd="true"
                android:rating="4" />

            <RatingBar
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ratingBar6"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_alignTop="@id/textView3"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                android:layout_alignParentEnd="true"
                android:rating="3" />
        </RelativeLayout>
    </LinearLayout>

</FrameLayout><!-- From: file:/home/gaurav/AndroidstudioProjects/Net%20app/app/src/main/res/layout/fragment_profile.xml -->